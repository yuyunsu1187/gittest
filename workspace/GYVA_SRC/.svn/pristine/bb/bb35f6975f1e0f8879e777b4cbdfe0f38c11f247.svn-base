@charset "utf-8";

/* 00.settion */
sub {display:inline-block;margin-top:-4px;vertical-align:top;}
/* 01.title */
.content-tit {font-size:5.55vw;width:100%;}
/* 03.button */
.history-btn {background-image:url("/resource/images/mobile/icon/icon-history-btn.png");height:4.44vw;padding:0 0;width:4.44vw;}
.instagram-link-btn {background-image:url("/resource/images/front/layout/img-instagram-link.png");height:8.88vw;width:43.88vw;}
.search-btn {height:4.44vw;padding:0 0;width:4.44vw;}
.sitemap-btn {background-image:url("/resource/images/mobile/icon/icon-menu-btn.png");height:4.44vw;width:4.44vw;}
.gotop-btn {background-color:rgba(255, 255, 255, 0.5);border-radius:50%;box-shadow:0 0 20px rgba(107, 107, 107, 0.3);color:#000000;font-size:3.33vw;font-weight:600;height:12.22vw;padding:0 0;position:absolute;right:4.44vw;bottom:3.88vw;width:12.22vw;z-index:10;}
.gotop-btn i {display:block;}
/* 12-01.board-wrap [data-board-type="form-list"] */
[data-board-type="form-list"] .link-btn:hover {color:#a50034;}
[data-board-type="form-list"] .link-btn:hover .link-txt {color:#6b6b6b;}
[data-board-type="form-list"] .link-btn .link-txt {color:#6b6b6b;}
/* 21. layout */
.default-section {height:100vh;}
.default-header {background-color:rgba(255, 255, 255, 0.55);display:flex;float:left;height:7.04vh;padding:2.77vw 4.16vw;position:fixed;left:0;top:0;right:0;z-index:10;}
.default-header .logo-tit {margin:0 auto;height:6.11vw;width:20vw;}
.default-header .logo-tit .link-btn {padding:0 0;width:100%;height:100%;}
.default-header .history-btn,
.default-header .search-btn,
.default-header .sitemap-tit {position:absolute;top:50%;transform:translateY(-50%);}
.default-header .history-btn {left:4.16vw;}
.default-header .search-btn {color:#a50034;font-size:5vw;right:13.88vw;}
.default-header .sitemap-tit {right:4.16vw;transform:translateY(-48vh);}
.default-header .form-btn {position:absolute;top:3.33vw;;}
.default-header [data-mult-form="search"] {background-color:#a50034;/* height:18.05vw;padding:3.33vw 5.55vw; */display:none;padding:0 5.55vw;position:fixed;top:7.04vh;left:0;right:0;}
.default-header [data-mult-form="search"] .input-form {padding-right:18.33vw;margin:3.33vw 0;}
.default-header [data-mult-form="search"] .form-btn {right:5.55vw;}
.default-contents {min-height:70vh;padding-top:7.04vh;padding-bottom:13.88vw;position:relative;}
.default-footer {background-color:#312e2d;font-size:2.77vw;height:30vh;padding:6.94vw 5.55vw;position:relative;text-align:center;}
.default-footer .menu-area .logo-tit {display:inline-block;height:4.44vw;width:48.33vw;}
.default-footer .menu-area .menu-list {margin:4.16vw auto;width:80%;}
.default-footer .menu-list .menu-btn {color:#b4b4b4;}
.default-footer .menu-list .menu-btn:hover,
.default-footer .menu-list .active .menu-btn {color:#ffffff;}
.default-footer .menu-list li {display:inline-block;}
.default-footer .menu-list li:nth-child(n+4) {margin-top:1.38vw;}
.default-footer .menu-list li + li .menu-btn:before {background-color:#545253;content:"";display:inline-block;width:0.27vw;height:1.11vw;margin:0 2.22vw;}
.default-footer [data-mult-form="select-list"] {color:#a7a9ac;font-size:3.33vw;margin-bottom:4.16vw;width:100%;}
.default-footer [data-mult-form="select-list"] i {color:#a7a9ac;}
.default-footer [data-mult-form="select-list"] .select-btn {background-color:#434141;border:none;}
.default-footer [data-mult-form="select-list"] .option-list {background-color:#434141;border:none;}
.default-footer [data-mult-form="select-list"] .option-list li + li {border-top:0.27vw solid #6b6b6b;}
/* layout : 컨텐츠 */
.content-section {padding:0 5.55vw;}
.content-header {align-items:center;background-image:url("/resource/images/mobile/contents/bg-header.png");background-position:center center;background-repeat:no-repeat;background-size:cover;color:#ffffff;display:flex;height:30vw;margin:0 -5.55vw;text-align:center;}
.content-header ~ :not(.tab-wrap) {margin-top:6.66vw;}
/* layout 메뉴 공통 */
.menu-list .menu-btn {padding:0 0;width:100%;text-align:left;}
.menu-list [data-menu-depth="2"] {display:none;}
/* 사이트 맵 : GNB */
.sitemap-wrap {background-color:rgba(0,0,0,0.9);position:fixed;top:0;right:0;bottom:0;transition:all .35s ease-in-out;} 
.sitemap-area {background-color:#ffffff;height:100vh;position:relative;width:77.5vw;transition:all .7s ease-in-out;z-index:5;}
.sitemap-area .instagram-link-btn {position:absolute;bottom:4.67vw;left:5.55vw;}
.sitemap-area .layerPop-close-btn {position:absolute;top:4.16vw;right:4.16vw;}
.sitemap-area .layerPop-close-btn .icon-close {height:6.11vw;margin-left:0;width:6.11vw;}
.sitemap-area .layerPop-close-btn .icon-close:before,
.sitemap-area .layerPop-close-btn .icon-close:after {background-color:#ffffff;}
.sitemap-area .layerPop-close-btn .icon-close:before {height:6.11vw;width:0.55vw;right:2.77vw;}
.sitemap-area .layerPop-close-btn .icon-close:after {height:0.55vw;width:6.11vw;top:2.77vw;}
.sitemap-area .menu-btn {color:#000000;font-size:4.44vw;font-weight:600;line-height:12.22vw;}
.sitemap-area .active .menu-btn {color:#a50034;}
.sitemap-area .member-info-area {background:#a50034 url("/resource/images/mobile/layout/bg-member-info.png") no-repeat bottom right;background-size:33.33%;color:#ffffff;padding:6.38vw 5vw;}
.sitemap-area .member-info-area .link-btn {padding:0 0;}
.sitemap-area .member-info-area .member-profile {display:block;width:6.94vw;height:6.94vw;margin-bottom:4.16vw;}
.sitemap-area .member-info-area .member-name {display:block;font-size:4.44vw;font-weight:600;margin-bottom:4.16vw;}
.sitemap-area .member-info-area .member-email {display:block;font-size:4.16vw;margin-bottom:6.11vw;}
.sitemap-area .member-info-area .logout-sub-btn {background-color:transparent;border:0.27vw solid #ffffff;border-radius:1.38vw;font-size:3.88vw;height:7.72vw;padding:0 0;width:25vw;}
.sitemap-area .menu-list {padding:5.55vw 5.55vw;height:57.03vh;overflow-y:auto;}
.sitemap-area .menu-list > li {border-bottom:0.27vw solid #dbdcdf;}
.sitemap-area [data-menu-depth="2"] {background-color:#f4f4f5;padding:2.77vw 5.55vw;border-top:0.27vw solid #dbdcdf;}
.sitemap-area [data-menu-depth="2"] .menu-btn {color:#6b6b6b;font-size:3.88vw;font-weight:400;line-height:10.27vw;}
.sitemap-wrap[data-pop-action="disabled"] {display:block;left:100%;}
.sitemap-wrap[data-pop-action="disabled"] .sitemap-tit {display:block;}
.sitemap-wrap[data-pop-action="disabled"] .sitemap-area {margin-left:100vw;}
.sitemap-wrap[data-pop-action="disabled"] .layerPop-close-btn {display:none;}
.sitemap-wrap[data-pop-action="active"] {left:0;}
.sitemap-wrap[data-pop-action="active"] .sitemap-tit {display:none;}
.sitemap-wrap[data-pop-action="active"] .sitemap-area {margin-left:22.5vw;}
.sitemap-wrap[data-pop-action="active"] .layerPop-close-btn {display:block;}

/* 네비게이션 메뉴 숨김 */
.breadcrumb {display:none;}
/* 더보기 버튼 숨김 */
.list-wrap .button-area {display:none;}
/* PC용 탭 리스트 */
.tab-wrap .tab-list {display:none;}
/* 상세페이지 */
.board-wrap .comment-list .profile-icon {display:none;}
.board-wrap .submit-area [data-offset] {display:none;}
.board-wrap .submit-area .main-btn {margin-left:0;}
.board-wrap .submit-area .main-btn i {display:none;}
.board-wrap .submit-area[data-useragent="PC"] {display:none;}

/* error */
.error-section {color:#000000;font-weight:600;text-align:center;position:absolute;top:50%;left:5.55vw;right:5.55vw;transform:translateY(-50%);}
.error-section .error-tit {font-size:5.55vw;}
.error-section .error-txt {font-size:3.05vw;}
.error-section .img-area {margin:7.5vw auto;width:17.36vw;}
.error-section .btn-area {margin-top:9.72vw;padding:0 0;width:100%;}
.error-section .btn-area [class*="btn"] {width:calc(50% - 1.11vw);font-size:3.88vw;height:11.11vw;line-height:11.11vw;}
/* main */
.visual-wrap {position:relative;}
.visual-wrap .visual-tit {color:#000000;font-size:8.33vw;font-weight:600;}
.visual-wrap .visual-tit sub {font-size:3.61vw;}
.visual-wrap .visual-txt {font-size:3.88vw;line-height:5vw;}
.visual-wrap .visual-txt sub {font-size:1.66vw;}
.visual-wrap * + .visual-txt {margin-top:6.94vw;}
.visual-wrap .visual-btn {background-color:rgba(255,255,255,0.3);border:0.27vw solid #000000;color:#000000;font-size:3.88vw;height:11.11vw;line-height:11.11vw;margin-top:20.88vw;width:44.44vw;text-align:left;}
.visual-wrap .visual-btn i {float:right;font-size:2.77vw;line-height:11.11vw;}
.visual-wrap .autoplay-btn {color:#302f2f;font-size:11px;height:11px;padding:0 0;position:absolute;bottom:56px;right:95px;width:11px;z-index:2;}
.visual-wrap .link-btn {padding:0 0;width:100%;}
.visual-wrap .text-area {position:absolute;left:8.33vw;top:25vw;text-align:left;z-index:2;}

.visual-wrap li:nth-child(1) .visual-txt {font-weight:600;line-height:6.94vw;}
.visual-wrap li:nth-child(1) .text-area {top:37.5vw;}
/* 메인 최근 게시물 영역 */
.post-wrap {position:relative;overflow:hidden;z-index:4;}
.post-wrap .link-btn {padding:0 0;}
.post-wrap .post-header {margin-bottom:4.44vw;padding:0 5.55vw;}
.post-header .post-tit {color:#000000;font-size:4.44vw;font-weight:600;}
.post-header .link-btn {float:right;font-size:3.33vw;margin-top:-4.44vw;}
.post-header .link-btn i {font-size:2.77vw;margin-left:1.66vw;}
.post-board {margin-top:11.11vw;}
.post-board .img-area {padding:0 5.55vw;}
.post-board .img-area .link-btn {border:0.27vw solid #e4e6e9;width:100%;overflow:hidden;}
.post-board .img-area .caption {padding-top:4.16vw;font-size:3.33vw;}
.post-board .img-area .caption * {display:block;}
.post-board .caption .tit {color:#201f1f;font-size:3.88vw;font-weight:600;}
.post-board .caption .host {color:#a50034;margin-top:2.77vw;}
.post-board .caption .date {display:block;margin-top:4.16vw;text-align:right;}
.post-board .caption .date:before {background:url("/resource/images/front/icon/icon_date.png") no-repeat center center;background-size:contain;content:"";display:inline-block;height:3.33vw;margin-right:2.22vw;margin-bottom:-0.27vw;width:3.33vw;}
.post-thumbnail {background-color:#fdfdfd;margin-top:11.11vw;padding:11.11vw 0;}
.post-thumbnail .thumbnail-list .link-btn {border:0.27vw solid #e4e6e9;width:100%;height:44.44vw;overflow:hidden;}
.post-thumbnail .thumbnail-list .link-btn:after {background:linear-gradient(to top, rgba(0,0,0,0.5) 20%, rgba(255,255,255,0) 100%);content:"";position:absolute;left:2.22vw;right:2.22vw;bottom:0;top:0;}
.post-thumbnail .thumbnail-list .caption {color:#ffffff;position:absolute;left:2.22vw;right:2.22vw;bottom:0;padding:4.16vw 4.44vw;text-align:left;z-index:2;}
.post-thumbnail .thumbnail-list .caption .tit {font-size:3.88vw;font-weight:600;}
.post-thumbnail .thumbnail-list .caption .txt {display:block;font-size:3.33vw;margin-top:2.77vw;}
.post-thumbnail .thumbnail-list .swiper-slide {padding:0 2.22vw;}
/* 메인 비주얼 스타일 */
[data-swiper-option="visual"] .swiper-container {height:63.59vh;}
[data-swiper-option="visual"] .swiper-container-horizontal>.swiper-pagination-bullets {bottom:8.33vw;}
[data-swiper-option="visual"] .swiper-pagination-bullet {background-color:#000000;border-radius:0;height:0.55vw;width:2.77vw;opacity:1;}
[data-swiper-option="visual"] .swiper-pagination-bullet-active {background-color:#a50034;width:5.55vw;margin-left:0 !important;}

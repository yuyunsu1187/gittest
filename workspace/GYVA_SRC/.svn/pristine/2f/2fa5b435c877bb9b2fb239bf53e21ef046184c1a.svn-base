@charset "utf-8";

/* 00.settion */
html, body {color:#6b6b6b;font-size:16px;font-weight:400;font-family:'LGSmHa',sans-serif;}
i {box-sizing:border-box;-moz-box-sizing:border-box;-webkit-box-sizing:border-box;-ms-box-sizing:border-box;}
.article-txt + *,
.article-tit + * {margin-top:36px;}
.article-tit + .article-txt {margin-top:15px;}

/* 10.box-area */
[data-offset="left"] {float:left;}
[data-offset="right"] {float:right;}
[data-offset="right"] + * {clear:both;}

/* 소단위 텍스트 영역 */
.text-area {}
.text-area .tit {color:#000000;font-size:16px;font-weight:600;}
.text-area .txt {color:#6b6b6b;font-size:13px;}
.text-area * + .txt {margin-top:30px;}
.text-area .txt + .txt {margin-top:10px;}

/* 페이징 영역 */
.paging-area {text-align:center;}
.paging-area > * {width:34px;height:34px;line-height:34px;vertical-align:middle;}
.paging-area strong {display:inline-block;}
.paging-area .paging-btn {padding:0 0;}

/* 일반버튼 영역 */
.button-area {margin-top:70px;text-align:center;}

/* no contents */
.noContents-area {color:#a7a9ac;font-size:22px;line-height:480px;height:530px;text-align:center;}

/* 개시물 전송버튼 영역 */
.submit-area {margin-top:40px;text-align:right;}
.submit-area [class*="btn"] {font-size:14px;height:50px;line-height:50px;min-width:210px;}
.submit-area [class*="btn"] + [class*="btn"] {margin-left:6px;}
.submit-area [class*="btn"] i {line-height:50px;}
.submit-area [class*="btn"] [class*="left"] {float:left;}
.submit-area [class*="btn"] [class*="right"] {float:right;}

/* 이미지 영역 */
.img-area {display:block;overflow:hidden;}
.video-area {overflow:hidden;}
.video-area video {width:100%;}

/* pdf 파일 미리보기 : type="application/pdf" */
.embed-area {display:block;width:100%;height:754px;margin:0 auto;}
.embed-area::-webkit-scrollbar {height:8px;width:8px;}
.embed-area::-webkit-scrollbar-track {background-color:#ebebeb;border-radius:4px;}
.embed-area::-webkit-scrollbar-thumb {background-color:#6b6b6b;border-radius:4px;}
.embed-area::-webkit-scrollbar-button {display:none;}

/* 키워드 태그 영역 */
/* .keywordTag-area {display:flex;flex-wrap:nowrap;} */
.keywordTag-area .txt {background-color:#ffffff;border:1px solid #a50034;border-radius:4px;display:inline-block;font-size:12px;height:25px;line-height:25px;max-width:100%;padding:0 10px;}
/* .keywordTag-area .txt + .txt {margin-left:4px;} */

/* 댓글 컴포넌트 */
.comment-list {}
.comment-list li + li {border-top:1px solid #dbdcdf;}
.comment-contents {padding:32px 0 32px 100px;position:relative;}
.comment-contents .name {color:#000000;font-size:15px;}
.comment-contents .datetime {font-size:13px;}
.comment-contents .profile-icon {display:inline-block;height:50px;width:50px;margin-right:20px;}
.comment-contents .condition-icon {border-radius:5px;display:inline-block;color:#ffffff;font-size:14px;margin-right:10px;min-width:70px;height:24px;line-height:24px;padding:0 12px;}
.comment-contents .text-area {color:#000000;display:inline-block;font-family:'LGSmHa',sans-serif;tab-size:0;line-height:24px;white-space:normal;width:calc(((100% / 6) * 5) - 5px);vertical-align:middle;}
.comment-contents .comment-header {font-weight:600;}
.comment-contents .comment-header * {vertical-align:middle;}
.comment-contents .comment-area {margin:20px 0;}
/* 사용자일 경우의 댓글영역 */
[data-comment-type="user"] {}
/* 인증강사일 경우의 댓글영역 */
[data-comment-type="certified-instructor"]  {}
[data-comment-type="certified-instructor"] [class*="btn"] {height:25px;line-height:25px;}
[data-comment-type="certified-instructor"] .list-sub-btn {margin:0 60px;width:calc((100% / 6) - 120px);}
[data-comment-condition="reply"] {background-color:#fbfafd;border-radius:10px;margin:0 calc(100% / 6) 32px 100px;padding:24px 45px;}
[data-comment-condition="reply"] [class*="btn"] {background-color:#a7a9ac;border-radius:3px;width:25px;padding:0 0;position:absolute;top:24px;}
[data-comment-condition="reply"] .modify-sub-btn {right:70px;}
[data-comment-condition="reply"] .delete-sub-btn {right:40px;}
[data-comment-condition="reply"] .text-area {width:100%;}
[data-comment-condition="reply"] .comment-area {margin-top:0;}
[data-comment-condition="reply"] .comment-area,
[data-comment-condition="reply"] .comment-footer {margin-left:74px;}
[data-comment-condition="waiting"] {background-color:#f36a00;}
[data-comment-condition="completed"] {background-color:#1347bd;}
/* 설문 리스트 */
[data-list-type="question-view"] [data-target-info] {flex:1;-ms-flex:1;padding:0 10px;}
[data-list-type="question-view"] .toggle-btn {display:flex;width:100%;padding:0 0;}
[data-list-type="question-view"] .question {padding:10px 20px;}
[data-list-type="question-view"] .question:before {margin-right:5px;}
[data-list-type="question-view"] .answer {background-color:#f5f5f5;padding:10px 45px;}
[data-list-type="question-view"] .list-area {border-bottom:1px solid #eee;}
[data-list-type="question-view"] .list-area li:nth-child(1) .question:before {content:"01.";}
[data-list-type="question-view"] .list-area li:nth-child(2) .question:before {content:"02.";}
[data-list-type="question-view"] .list-area li:nth-child(3) .question:before {content:"03.";}
[data-list-type="question-view"] .list-area li:nth-child(4) .question:before {content:"04.";}
[data-list-type="question-view"] .list-area li:nth-child(5) .question:before {content:"05.";}
[data-list-type="question-view"] .list-area li:nth-child(6) .question:before {content:"06.";}
[data-list-type="question-view"] .list-area li:nth-child(7) .question:before {content:"07.";}
[data-list-type="question-view"] .list-area li:nth-child(8) .question:before {content:"08.";}
[data-list-type="question-view"] .list-area li:nth-child(9) .question:before {content:"09.";}
[data-list-type="question-view"] .list-area li:nth-child(10) .question:before {content:"10.";}

/* 토글 */
.toggle-section {}
.toggle-header {}
.toggle-content {} /* 설정된 액션에 따라 수행되는 연결 영역 */  
.toggle-btn {} /* 토글 액션 수행 */
[data-type="toggle"] {} /* 토글을 검색하는 data- 값 */
[data-toggle-action] /* 수행할 액션 갑 : slide, animate, menu */
[data-slide-action] /* slide 액션의 설정 : slideUp, slideDown */
[data-animate-action] /* slide 액션의 설정 : passive, active */

/* 11.tab-wrap */
.tab-wrap {}
.tab-list {margin-bottom:50px;}
.tab-list {background-color:#f4f4f5;border-radius:8px;overflow:hidden;}
.tab-list li {float:left;}
.tab-list li + li:before {background-color:#c6c8ca;content:"";float:left;height:14px;margin-top:18px;width:1px;}
.tab-list .list-btn {font-size:18px;height:50px;line-height:50px;padding:0 0;width:calc(100% - 1px);text-align:center;}
.tab-list [data-tab-action="active"] {background-color:#a50034;border-radius:8px;color:#ffffff;}
.tab-list [data-tab-action="active"]:before,
.tab-list [data-tab-action="active"] + li:before {display:none;}
/* 
.tab-contents [data-combine-list="column-4-list"] .img-area {height:406px;} 
.tab-contents [data-combine-list="column-4-list"] .img-area .caption {height:215px;}
*/
/* 탭 개수에 따른 너비 설정 */
[data-combine-tab="column-3-tab"] li {width:calc(33.33%);}
[data-combine-tab="column-4-tab"] li {width:calc(25%);}
[data-combine-tab="column-5-tab"] li {width:calc(20%);}
[data-tab-action="hide"] {display:none;}
[data-tab-action="show"] {display:block;}

/* 12.board-wrap */
.board-wrap {}
.board-wrap .article-txt {margin-left:10px;}
.board-wrap .board-main-btn {height:34px;line-height:34px;}
.board-wrap .board-area + .board-area,
.board-wrap .board-area + .article-tit {margin-top:77px;}
.board-wrap .paging-area {margin-top:20px;}
.board-wrap .board-header {background-color:#e4e6e9;font-size:16px;line-height:30px;padding:20px 60px;}
.board-header > * {display:inline-block;vertical-align:middle;}
.board-header .board-tit {position:relative;padding-right:40px;width:calc(100% - 135px);}
.board-header .board-tit:before {background-color:#a7a9ac;width:1px;position:absolute;top:6px;bottom:6px;left:60px;}
.board-header .download-main-btn {width:130px;height:40px;line-height:40px;}
.board-wrap .keywordTag-area {margin:20px 40px;}
.board-wrap .keywordTag-area .txt {color:#a50034;}
.board-wrap .board-content {background-color:#ffffff;border-bottom:1px solid #dbdcdf;padding:60px 60px;}
.board-content .img-area {max-width:750px;margin:0 auto;}
.board-content * + .text-area {margin-top:50px;}
/* board-wrap > 댓글 리스트 */
.board-content [data-combine-form="column-6-form"] {padding:32px 0 32px 100px;}
.board-content .comment-list {background-color:#ffffff;border-top:1px solid #6b6b6b;border-bottom:1px solid #6b6b6b;}
.board-content .comment-contents .text-area {font-size:16px;}
/* board-wrap > 설문 리스트 */
.board-wrap [data-list-type="question-view"] {padding:30px 0 0;border-bottom:none;}
.board-wrap [data-list-type="question-view"] > li + li .toggle-btn [data-target-info] {border-top:none;}
.board-wrap [data-list-type="question-view"] .toggle-btn [data-target-info] {border:1px solid #dbdcdf;height:34px;line-height:34px;}
.board-wrap [data-list-type="question-view"] .toggle-btn [data-target-info] + [data-target-info] {border-left:none;}
.board-wrap .list-section [data-list-type="question-view"] {padding-top:0}
.board-wrap .list-section [data-list-type="question-view"] > li {border-bottom:none;}
.board-wrap .list-section .toggle-header [data-target-info] [class*="btn"] {margin-top:4px;}
.board-wrap .list-section .toggle-content {border-bottom:1px solid #dbdcdf;}
/* 12-01.board-wrap [data-board-type="form-list"] */
[data-board-type="form-list"] {}
[data-board-type="form-list"] [data-board-type="inquiry"] {margin-bottom:20px;overflow:hidden;}
[data-board-type="form-list"] [data-board-type="inquiry"] .form * {height:40px;line-height:40px;}
[data-board-type="form-list"] [data-board-type="inquiry"] [data-combine-form="column-2-form"] {float:left;width:50%;}
[data-board-type="form-list"] [data-board-type="inquiry"] [data-combine-form="column-2-form"] + *,
[data-board-type="form-list"] [data-board-type="inquiry"] [data-offset="right"] {text-align:right;}
[data-board-type="form-list"] [data-board-type="inquiry"] [data-offset="right"] {float:right;}
[data-board-type="form-list"] [data-board-type="inquiry"] [data-combine-form="column-3-form"] {width:50%;}
[data-board-type="form-list"] [data-text-style="ellipsis"] {display:table-cell;text-align:left;}
[data-board-type="form-list"] [data-text-style="ellipsis"] * {display:inline;}
[data-board-type="form-list"] [data-td-type="board-noData"] {height:250px;}
/* 12-02.board-wrap [data-board-type="form-write"] */
[data-board-type="form-write"] th,
[data-board-type="form-write"] td {padding:12px 12px;text-align:left;vertical-align:top;}
[data-board-type="form-write"] [readonly] {/* height:auto; */}
[data-board-type="form-write"] .note-txt + *,
[data-board-type="form-write"] * + .img-area {margin-top:18px;}
/*  data-radio-key값에 연결된 tr의 key값 정의 :  라디오 선택값에 따라 노출여부 변경, 두 값은 동일하게 설정  */
/* [data-board-type="form-view"] [data-row-key] {display:none;}*/
/* 12-03.board-wrap [data-board-type="form-view"] : 상단 타입 분리 */
[data-board-type="form-view"] {}
[data-board-type="form-view"] [data-board-header="download"] .category-txt {width:175px;}
[data-board-type="form-view"] [data-board-header="basic"] {background-color:#f6e5ea;}
[data-board-type="form-view"] [data-board-header="comment"] {background-color:#f6e5ea;padding:20px 40px;}
[data-board-type="form-view"] [data-board-header="comment"] .board-tit {padding:0 0;width:100%;}
[data-board-type="form-view"] [data-type="swiper"] {padding:60px 145px;}
[data-board-type="form-view"] [data-type="swiper"] .img-area {max-width:100%;}
[data-board-type="form-view"] .swiper-container {height:580px;padding:0 120px 15px;}
[data-board-type="form-view"] .swiper-container .swiper-pagination-progressbar {left:50%;width:calc(100% - 240px);transform:translateX(-50%);}
[data-board-type="form-view"] .board-detail {font-size:15px;}
[data-board-type="form-view"] .board-detail .list {padding:30px 340px 30px 0;min-height:200px;}
[data-board-type="form-view"] .board-detail .list li {border:none;padding:10px 0;}
[data-board-type="form-view"] .board-detail .list li * {vertical-align:top;padding:0 0;}
[data-board-type="form-view"] .board-detail .list li *:before {content:"";display:inline-block;}
[data-board-type="form-view"] .board-detail .list .tit {color:#a50034;width:220px;padding-left:40px;}
[data-board-type="form-view"] .board-detail .list .tit:before {background-color:#a50034;height:3px;margin-right:5px;width:3px;}
[data-board-type="form-view"] .board-detail .list .txt {margin-left:40px;width:calc(100% - 280px);text-indent:-40px;}
[data-board-type="form-view"] .board-detail .list .txt:before {background-color:#a7a9ac;height:12px;margin-right:40px;width:1px;}
[data-board-type="form-view"] .board-detail .img-wrap {float:right;}
[data-board-type="form-view"] .board-detail .bookmark-btn {border:2px solid #a7a9ac;border-radius:20px;height:40px;min-width:80px;margin-top:20px;margin-right:20px;vertical-align:top;}
[data-board-type="form-view"] .board-detail .bookmark-btn:before {background-image: url(/resource/images/common/icon/icon-bookmark.png);content:"";display:inline-block;width:18px;height:18px;margin-right:8px;}
[data-board-type="form-view"] .board-detail .bookmark-btn.active{border:2px solid transparent;background:linear-gradient(to right, white, white), linear-gradient(135deg, #f799b2 , #b52450);background-clip:padding-box, border-box;background-origin:padding-box, border-box;}
[data-board-type="form-view"] .board-detail .bookmark-btn.active:before {background-image: url(/resource/images/common/icon/icon-bookmark-active.png);}
[data-board-type="form-view"] .board-detail .img-area {display:inline-block;width:200px;}
[data-board-type="form-view"] .board-detail + .board-content {padding:10px 0;}
[data-board-header="basic"] .board-tit {padding:0 40px 0 100px;width:calc(100% - 214px);}
[data-board-header="basic"] .board-tit:before {content:"";}
[data-board-header="basic"] .category-txt {background-color:#a50034;width:105px;}
[data-board-header="basic"] .date-txt {width:100px;}

/* 13.list-wrap */
.list-wrap {font-size:13px;}
.list-wrap + .list-wrap {margin-top:60px;}
.list-wrap .link-btn {display:block;padding:0 0;}
.list-wrap .keywordTag-area .txt {background-color:#e4e6e9;border:none;font-size:11px;height:20px;line-height:20px;}
.list-wrap .img-area {box-shadow:0px 0px 15px rgba(107, 107, 107, 0.05);/* height:285px; */width:100%;}
.list-wrap .img-area .link-btn {text-align:left;}
.list-wrap .img-area .caption {background-color:#ffffff;display:block;/* height:95px; */padding:26px 20px;position:relative;text-align:left;}
.list-wrap .img-area .caption .tit {color:#000000;font-size:18px;font-weight:600;}
/* .list-wrap .img-area .caption .txt {color:#6b6b6b;font-size:13px;} */
.list-wrap .img-area .caption .data {background-color:#fbfbfc;overflow:hidden;color:#483e3e;font-size:14px;line-height:40px;margin-top:26px;padding-left:20px;}
.list-wrap .img-area .caption .data .date:after {background-color:#dbdcdf;content:"";display: inline-block;height:8px;margin:0 15px;width:1px;}
.list-wrap .img-area .caption .data .download-main-btn {float:right;}
.list-wrap .img-area .bookmark > .tit {padding-right:45px;}
.list-wrap .img-area .bookmark .bookmark-btn {background-image:url("/resource/images/common/icon/icon-bookmark.png");height:28px;width:28px;position:absolute;right:24px;top:33px;}
.list-wrap .img-area .bookmark-btn.active {background-image:url("/resource/images/common/icon/icon-bookmark-active.png");}
.list-wrap .img-area .profile {padding:12px 8px 0;}
.list-wrap .img-area .profile .img {border-radius:50%;display:inline-block;overflow:hidden;}
.list-wrap .img-area .profile .img img {height:100%;}
.list-wrap .img-area .profile .tit {color:#494444;font-size:14px;line-height:20px;height:40px;}
.list-wrap .img-area .profile .txt {font-size:12px;margin-top:10px;line-height:16px;height:48px;}
.list-wrap .img-area .download-main-btn {background:linear-gradient(to right, rgba(0,0,0,0.7), rgba(0,0,0,0.7));height:40px;line-height:40px;width:130px;}
.list-wrap .img-area .download-main-btn i {line-height:32px;}
.list-wrap .img-area .category-txt {background-color:#a7a9ac;font-size:11px;height:20px;line-height:20px;padding:0 0;position:absolute;right:12px;top:12px;width:40px;}
.list-wrap .img-area,
.list-wrap .img-area .caption,
.list-wrap .img-area .caption .tit,
.list-wrap .img-area .caption:before,
.list-wrap .img-area .caption:after,
.list-wrap .img-area .download-main-btn {transition:all .35s ease-in-out;}
/* .list-wrap .img-area .caption, */
.list-wrap .img-area .caption:before,
.list-wrap .img-area .caption:after {position:absolute;}
.list-wrap .img-area:hover {box-shadow:0px 0px 15px rgba(107, 107, 107, 0.3);}
.list-wrap .img-area:hover .link-btn,
.list-wrap .link-btn:hover {text-decoration:none;}
.list-wrap .img-area:hover .caption:before,
.list-wrap .img-area:hover .caption:after,
.list-wrap .img-area:hover .profile:after {background-color:#a50034;}
.list-wrap .img-area:hover .caption .tit {color:#a50034;}
.list-wrap .img-area:hover .profile .tit {color:#494444;}
.list-wrap .img-area:hover .download-main-btn {background:linear-gradient(to right, #a50034, #702076);}
.list-wrap .list-header {margin-bottom:36px;}
/* Recent(최근순) Popular(인기순) : 탭 > 동일한 UI의 정렬은 list-header영역에 sort-list배치, 배치 방식에 따라 디자인이 변경 됨 */
.list-header .sort-list {float:right;margin-top:-26px;}
.list-header .sort-list li {width:auto;float:none;}
.list-header .sort-list li + li {margin-left:0;}
.list-header .sort-list li + li:before {background-color:#a7a9ac;content:"";display:inline-block;height:8px;margin:0 12px;width:1px;}
.list-header .sort-list .sort-btn {padding:0 0;}
/* .list-wrap > PDF 파일 리스트 */
.list-wrap .pdf-list .img-area {height:520px;width:100%;}
.list-wrap .pdf-list .img-area .link-btn {border:1px solid #e6e7ea;width:340px;left:40px;top:40px;bottom:40px;}
.list-wrap .pdf-list .img-area .img {display:block;border:1px solid #e6e7ea;width:340px;left:40px;top:40px;bottom:auto;}
.list-wrap .pdf-list .img-area .caption {padding:40px 40px;}
.list-wrap .pdf-list .img-area .tit {background-color:#f1f2f4;display:block;margin:0 -40px;padding:36px 40px 36px 405px;}
.list-wrap .pdf-list .img-area .txt {margin:20px 0 40px 365px;height:42px;line-height:21px;text-decoration:underline;}
.list-wrap .pdf-list .img-area .sub-btn {border-radius:24px;font-size:14px;height:45px;line-height:45px;position:absolute;bottom:60px;left:405px;width:135px;}
.list-wrap .pdf-list .img-area .sub-btn i {font-size:10px;margin-left:4px;}
.list-wrap .pdf-list .img-area:hover .txt {color:#6b6b6b;}
.list-wrap .pdf-list .img-area:hover .tit {background-color:#a50034;color:#ffffff;}
.list-wrap .pdf-list .img-area:hover .sub-btn {background-color:#a50034;}
/* .list-wrap >  1:1 문의 리스트 */
/* All(전체) Submitted(제출) Completed(완료) : section > 정렬은 list-section영역에 sort-list 배치, 배치 방식에 따라 디자인이 변경 됨 */
.list-section .sort-list {border-bottom:1px solid #a7a9ac;margin-bottom:20px;}
.list-section .sort-list .sort-btn {min-width:75px;padding:0 14px;}
.list-section .sort-list .active .sort-btn,
.list-section .sort-list .sort-btn:hover {border-bottom:2px solid #a50034;}
.list-section .section-header {background-color:#f6e5ea;color:#494444;display:flex;font-size:14px;font-weight:600;}
.list-section .section-header [data-target-info] {height:40px;line-height:40px;}
.list-section .toggle-header [data-target-info] {height:50px;line-height:50px;}
.list-section .toggle-header [data-target-info] [class*="btn"] {float:right;height:25px;line-height:25px;margin-top:12.5px;width:100px;}
.list-section .toggle-header [data-target-info="title"] {color:#a50034;text-align:left;}
.list-section .toggle-content {background-color:#fefcfd;border-top:1px solid #a50034;}
.list-section .comment-wrap {padding:20px 40px;}
.list-section .comment-contents {padding:0 0;}
.list-section .comment-contents:before {content:"Q";display:inline-block;color:#a50034;font-weight:600;margin-right:40px;vertical-align:top;}
.list-section [data-target-info] {flex:1;-ms-flex:1;text-align:center;}

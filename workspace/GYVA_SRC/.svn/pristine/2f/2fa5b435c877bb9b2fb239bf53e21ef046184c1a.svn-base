.text-area .tit {font-size:3.88vw;}
.text-area .txt {font-size:3.88vw;}
.text-area * + .txt {margin-top:6.94vw;}
.text-area .txt + .txt {margin-top:2.77vw;}

/* 일반버튼 영역 */
.button-area {margin-top:70px;}

/* 개시물 전송버튼 영역 */
.submit-area {margin-top:11.11vw;}
.submit-area [class*="btn"] {font-size:3.88vw;height:11.11vw;line-height:11.11vw;min-width:auto;width:100%;}
.submit-area [class*="btn"] + [class*="btn"] {margin-left:0.83vw;}

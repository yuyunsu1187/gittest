#AD030501-05 [data-board-type="inquiry"] {overflow:inherit;}
#AD030501-05 [data-board-type="inquiry"] .sort-list {margin-left:4px;}
#AD030501-05 [data-board-type="inquiry"] [data-combine-form="column-2-form"] {float:right;margin-top:-50px;text-align:right;}
/* id="AD050101-05" : Branding Material > Logo */

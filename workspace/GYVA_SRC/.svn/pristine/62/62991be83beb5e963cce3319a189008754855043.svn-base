/* id="AD050903-05" : 설문관리 > 설문상세 목록 */
#AD050903-10 .board-header .board-tit {width:calc(100% - 314px);padding:0 40px 0 100px;}
#AD050903-10 .board-header .board-tit:before {content:"";}
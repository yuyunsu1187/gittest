/* 03.button */
[class*="btn"], a[class*="btn"] {padding:0 5.55vw;}
[class*="main-btn"], a[class*="main-btn"],
[class*="sub-btn"], a[class*="sub-btn"] {border-radius:1.38vw;}
.bookmark-btn {padding:0 0;}
.download-main-btn, a.download-main-btn {font-size:3.88vw;}
.download-main-btn i {line-height:3.88vw;margin-left:2.5vw;}

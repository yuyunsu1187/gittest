/* 04.icon */	
	
/* 05.form */
.form li + li {margin-left:20px;}
.form + .form {margin-top:1.66vw;}
.form [class*="btn"] {font-size:3.88vw;height:11.11vw;line-height:11.11vw;}
.form .icon-btn {font-size:4.16vw;width:11.11vw;}
.input-form {border-width:0.27vw;border-radius:1.38vw;font-size:3.88vw;height:11.11vw;line-height:11.11vw;min-width:auto;padding:0 3.33vw;}
.input-form.invalid, .input-form.invalid:hover, .input-form.invalid:focus,
.select-form.invalid, .select-form.invalid:hover, .select-form.invalid:focus {border-width:0.27vw;}
textarea.input-form {padding:10px 10px;min-height:128px;}
.select-form {border-width:0.27vw;border-radius:1.38vw;height:11.11vw;line-height:11.11vw;padding:0 20px;} 
.label-form {color:#000000;display:inline-block;font-size:3.33vw;line-height:5.55vw;padding-left:6.94vw;}
.label-form:before {height:5vw;width:5vw;}
/* 첨부파일 폼 */
.file-label {border-radius:1.38vw;font-size:3.88vw;height:11.11vw;line-height:11.11vw;}
/* .form data- 속성, 소단위 요소 정의 */
[data-form="select"] {min-width:120px;}
[data-form="select"] i {font-size:16px;height:11.11vw;line-height:11.11vw;right:17px;}
[data-form="checkbox"] .label-form:after {height:5vw;width:5vw;}
[data-form="checkbox"] .label-form:before {border-radius:3px;}
[data-form="radio"] .label-form:before {border-width:0.27vw;}
[data-form="radio"] .label-form:after {width:2.77vw;height:2.77vw;top:1.38vw;left:1.38vw;}

/* 05-01.form-wrap, -area */
.form-wrap + .form-wrap {margin-top:11.11vw;}
.form-area > li + li {padding-top:6.66vw;}
.form-area .form-tit {display:block;font-size:3.33vw;min-width:auto;}
.form-area .form-tit + .form {width:100%;margin-top:3.33vw;}
/* 05-02.mult-form */
[data-mult-form="search"] .input-form,
[data-mult-form="password"] .input-form,
[data-mult-form="date"] .input-form {padding-right:50px;}
/* 디자인 된 선택박스 : data-mult-form="select-list" */
[data-mult-form="select-list"][data-select-offset="bottom"] .option-list {bottom:11.38vw;}
[data-mult-form="select-list"][data-select-offset="top"] .option-list {top:11.38vw;}
[data-mult-form="select-list"] i {width:12.5vw;}
[data-mult-form="select-list"] .select-btn {border-width:0.27vw;border-radius:1.38vw;}
[data-mult-form="select-list"] .select-btn.invalid,
[data-mult-form="select-list"] .select-btn:hover, 
[data-mult-form="select-list"] .select-btn:focus {border-width:0.27vw;}
[data-mult-form="select-list"] .option-list {border-width:0.27vw;border-radius:1.38vw;left:0;right:0;max-height:56.66vw;}
[data-mult-form="select-list"] .option-list li + li {border-width:0.27vw;}
/* 파일 찾기 폼 영역 : data-mult-form="file" */
[data-mult-form="file"] .file-label {margin-left:0;min-width:22.77vw;}
[data-mult-form="file"] .input-form {width:calc(100% - 24.16vw);}
[data-mult-form="comment"] .input-form {min-height:19.44vw;}
/* input-btn : input + button 조합 */
[data-mult-form="input-btn"] .input-form {width:calc(100% - 25.55vw);}
[data-mult-form="input-btn"] [class*="btn"] {margin-left:1.11vw;width:23.05vw;}
/* 05-03.combine-form */
/* 폼 영역의 다단 스타일 적용 */
[data-combine-form="column-2-form"] .form {width:calc(50% - 7px);}
[data-combine-form="column-2-form"] [data-form="select"] {width:calc(30% - 8px);margin-right:4px;}
[data-combine-form="column-2-form"] [data-mult-form="search"] {width:400px;}
[data-combine-form="column-3-form"] .form {width:calc(33.33% - 4px);}
[data-combine-form="column-6-form"] [data-mult-form="comment"] {width:calc(((100% / 6) * 5) - 5px);}
[data-combine-form="column-6-form"] [data-mult-form="btn"] {width:calc(100% / 6);padding:0 30px;}
/* 파일추가 리스트 */
[data-combine-form="file-list"] [data-mult-form="file"] {width:calc(100% - 76px);}
[data-combine-form="file-list"] [data-mult-form="file-btn"] {width:72px;}
/* 날짜선택 폼 */
[data-combine-form="datepicker"] {}
[data-combine-form="datepicker"] [data-mult-form="date"] {min-width:220px;width:25%;}
[data-combine-form="datepicker"] .symbol {width:20px;}
/* 로그인 폼 영역 */
[data-combine-form="login-form"] {border-width:0.27vw;}
/* 설문등록 리스트 */
[data-combine-form="question-form"] > li {padding:12px 20px;}
[data-combine-form="question-form"] > li + li {border-width:0.27vw;}

/* 06.list */
.sort-list li + li {margin-left:-1.11vw;}
.sort-list .sort-btn {font-size:3.33vw;height:7.22vw;line-height:7.22vw;}

/* 07.table */
.default-table th, td {border-width:0.27vw;min-height:40px;padding:12px 10px;}
}

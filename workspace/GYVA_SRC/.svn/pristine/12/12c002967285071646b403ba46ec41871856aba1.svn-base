.article-tit {font-size:4.44vw;line-height:8.33vw;} /* [3hepth] content > article 타이틀 */
.article-tit:before {height:4.44vw;width:4.44vw;margin-right:2.77vw;}
.board-tit {font-size:17px;} /* [3hepth] content > 게시물 타이틀 */

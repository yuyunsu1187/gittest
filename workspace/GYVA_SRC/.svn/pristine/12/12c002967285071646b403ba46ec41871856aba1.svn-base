.terms-main-btn {background-color:#dbdcdf;border-radius:0;color:#494444;font-size:3.33vw;min-height:9.72vw;padding:2.77vw 13.88vw 2.77vw 5.55vw;position:relative;width:100%;text-align:left;}
.terms-main-btn i {position:absolute;top:0;right:0;height:9.72vw;line-height:9.72vw;width:13.88vw;text-align:center;}
